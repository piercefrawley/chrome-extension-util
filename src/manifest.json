{
  "name": "Home Page",
  "version": "1.0",
  "description": "Home Page Extension",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "page": "background.html"
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "get_started128.png"
    },
    "icons": {
      "128": "get_started128.png"
    }
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
